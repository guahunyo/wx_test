<swiper indicator-dots="true" indicator-color="rgba(0, 0, 0, .3)" indicator-active-color="#c33" autoplay="true" interval="3000" duration="500">
	<block wx:for="{{imgUrls}}">
		<swiper-item>
			<image src="{{item}}" class="slide-image" width="750rpx" height="300"/>
		</swiper-item>
	</block>
</swiper>	
